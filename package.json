{
  "name": "openskie",
  "version": "1.0.0",
  "description": "continuing education tracker for teachers and childcare workers",
  "main": "index.js",
  "bin": {
    "openskie": "bin/cmd.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "loose": [
            "es6.modules"
          ],
          "jsxPragma": "h",
          "plugins": [
            "babel-plugin-jsx-factory"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "start": "babel-node bin/cmd.js",
    "test": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/openskie/openskie.git"
  },
  "keywords": [
    "continuing",
    "education",
    "app",
    "teachers"
  ],
  "bugs": {
    "url": "https://github.com/openskie/openskie/issues"
  },
  "homepage": "https://github.com/openskie/openskie#readme",
  "dependencies": {
    "babel": "^5.8.23",
    "babel-plugin-jsx-factory": "^1.0.1",
    "body": "^5.1.0",
    "ecstatic": "^1.0.0",
    "hyperstream": "^1.2.2",
    "inherits": "^2.0.1",
    "is-root": "^1.0.0",
    "main-loop": "^3.2.0",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mysql": "^2.9.0",
    "routes": "^2.1.0",
    "sqlite3": "^3.1.0",
    "tcp-bind": "^1.2.0",
    "virtual-dom": "^2.1.1",
    "websocket-stream": "^2.0.2",
    "xtend": "^4.0.0"
  }
}
